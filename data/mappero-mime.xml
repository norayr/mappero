<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info"
           xmlns:osso="http://nokia.com/osso/mime-categories">
  <mime-type type="application/vnd.google-earth.kml+xml">
    <comment>KML map document</comment>
    <osso:category name="other"/>
    <magic priority="50">
      <match value="kml" type="string" offset="0:160"/>
    </magic>
    <glob pattern="*.kml"/>
  </mime-type>
  <mime-type type="application/gpx+xml">
    <comment>GPX map document</comment>
    <osso:category name="other"/>
    <magic priority="50">
      <match value="gpx" type="string" offset="0:160"/>
    </magic>
    <glob pattern="*.gpx"/>
  </mime-type>
</mime-info>
